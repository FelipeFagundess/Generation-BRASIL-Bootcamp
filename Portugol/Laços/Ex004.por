programa
{
	
	funcao inicio()
	{
	 inteiro num1
	 escreva("Digite um numero:")
	 leia(num1)
	 enquanto(num1 <=100){
	 	num1=num1 * 3
	 	escreva(" ", num1)
	 	}
	}
}
